{
  "name": "bambu",
  "version": "0.7.1-beta.4",
  "description": "",
  "main": "lib/index.js",
  "typings": "lib/index",
  "scripts": {
    "start": "node ./tools/app/start.js",
    "watch:react-docs": "node --max_old_space_size=4096 ./tools/FileWatcher.js",
    "start:app": "webpack-dev-server --port 8383 --config tools/app/dev/webpack.js --colors",
    "showcase:build": "rimraf ./docs && node ./tools/app/build.js",
    "showcase:serve": "http-server ./docs -p 8282",
    "build:app": "webpack --config tools/app/build/webpack.js --colors",
    "build": "rimraf ./lib && node ./tools/lib/build_lib.js",
    "build:lib": "tsc -p ./tsconfig-lib.json",
    "copy:lib": "node ./tools/copyFiles.js",
    "test": "jest",
    "export:modules": "node ./tools/findAndExportModules.js",
    "export:bulma-css-as-json": "gulp bulma-css-as-json",
    "export:react-docs": "rimraf ./showcase/modules/react-docs && node ./tools/parseReactDoc.js"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/chai": "^4.1.4",
    "@types/chai-enzyme": "^0.6.5",
    "@types/classnames": "^2.2.6",
    "@types/codemirror": "^0.0.59",
    "@types/enzyme": "^3.1.13",
    "@types/history": "^4.7.0",
    "@types/jest": "^23.3.1",
    "@types/node": "^10.7.1",
    "@types/prop-types": "^15.5.5",
    "@types/react": "^16.4.11",
    "@types/react-dom": "^16.0.7",
    "@types/react-router-dom": "^4.3.0",
    "@types/systemjs": "^0.20.6",
    "app-root-path": "^2.1.0",
    "bulma": "^0.7.1",
    "chai": "^4.1.2",
    "chai-enzyme": "^1.0.0-beta.1",
    "classnames": "^2.2.6",
    "codemirror": "^5.39.2",
    "colors": "^1.3.1",
    "copy-webpack-plugin": "^4.5.2",
    "css-loader": "^1.0.0",
    "enzyme": "^3.4.4",
    "enzyme-adapter-react-16": "^1.2.0",
    "es6-promise": "^4.2.4",
    "file-extension": "^4.0.5",
    "history": "^4.7.2",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^23.5.0",
    "js-criteria": "^2.1.2",
    "mobx": "^5.0.4",
    "mobx-react": "^5.2.5",
    "prop-types": "^15.6.2",
    "react": "^16.4.2",
    "react-docgen-typescript": "^1.7.0",
    "react-dom": "^16.4.2",
    "react-router-dom": "^4.3.1",
    "rimraf": "^2.6.2",
    "style-loader": "^0.22.1",
    "ts-loader": "^4.5.0",
    "tslint": "^5.11.0",
    "tslint-config-airbnb": "^5.10.0",
    "typescript": "^3.0.1",
    "webpack": "^4.17.0",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.5",
    "webpack-notifier": "^1.6.0"
  },
  "peerDependencies": {
    "bulma": ">=0.7.1",
    "classnames": ">=2",
    "font-awesome": ">=5",
    "prop-types": ">=15",
    "react": ">=15"
  },
  "dependencies": {
    "wasabi-common": "^2.5.0"
  },
  "babel": {
    "presets": [
      "env",
      "react",
      "stage-2"
    ]
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/tools/jest/test-shim.js",
      "<rootDir>/tools/jest/test-setup.js"
    ],
    "collectCoverageFrom": [
      "**/src/**/*.(ts|tsx)"
    ],
    "coverageReporters": [
      "text-summary",
      "html"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "verbose": true,
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/tools/jest/test-preprocessor.js"
    },
    "testMatch": [
      "**/__tests__/**/*.spec.(ts|tsx)"
    ]
  }
}
