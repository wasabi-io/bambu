{
  "extends": "tslint-config-airbnb",
  "rules": {
    "no-console": [
      false
    ],
    "ter-indent": [
      true,
      2
    ],
    "import-name": false,
    "interface-name": false,
    "max-line-length": [
      250
    ],
    "max-classes-per-file": [
      false
    ],
    "no-var-requires": false,
    "trailing-comma": false
  },
  "jsRules": {}
}