{
  "extends": "tslint-config-airbnb",
  "rules": {
    "object-curly-spacing": ["error", "never"],
    "no-console": [
      false
    ],
    "no-eval": false,
    "ter-indent": [
      false,
      4
    ],
    "align": [
      true,
      "elements",
      "members",
      "parameters",
      "statements"
    ],
    "quotemark": [false],
    "variable-name": [
      true,
      "allow-pascal-case",
      "check-format",
      "allow-leading-underscore"
    ],
    "import-name": false,
    "interface-name": false,
    "max-line-length": [
      true,
      1000
    ],
    "max-classes-per-file": [
      false
    ],
    "no-var-requires": false,
    "trailing-comma": false
  },
  "jsRules": {}
}