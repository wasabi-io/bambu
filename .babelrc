{
  "presets": [
    [
      "env",
      {
        "targets": {
          "uglify": true
        },
        "modules": false
      }
    ],
    "react"
  ],
  "plugins": [
    "transform-class-properties",
    "transform-object-rest-spread",
    "syntax-dynamic-import"
  ],
  "env": {
    "test": {
      "presets": ["env", "react"],
      "plugins": [
        "system-import-transformer",
        "transform-class-properties",
        "transform-object-rest-spread"
      ]
    }
  }
}